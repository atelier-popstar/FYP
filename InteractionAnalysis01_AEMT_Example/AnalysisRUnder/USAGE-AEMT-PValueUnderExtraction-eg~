###Create the first 10 files
### place the 10 to 16 in the folder perlScript
cd /Desktop/AEMT/InteractionAnalysis_AEMT/AnalysisR_UnderChance

## Rename inside files
sed -i 's/Levelaemt/LevelUnderaemt/g' *
## Rename files name
#rename  's/Levelaemt/LevelUnderaemt/' *

## Create SS / N1 / N2 extraction Files
##N1OS
sed -i 's/>PValues/>PValuesN1/g' *
sed -i 's/_OS/N1_OSN1/g' *
rename  's/_OS/_OSN1/' *

sed -i 's/>PValuesN1/>PValuesN2/g' *
sed -i 's/N1_OSN1/N2_OSN2/g' *
rename  's/_OSN1/_OSN2/' *

sed -i 's/OS/SS/g' *
rename  's/OS/SS/' *

sh executionPerlScripts.sh

cat pValuesaemt**_OS.txt > pValuesaemtOS.txt
cat pValuesaemt**_SS.txt > pValuesaemtSS.txt

cat pValuesaemt**N1_**N1.txt > pValuesaemtN1.txt

cat pValuesaemt**N2_**N2.txt > pValuesaemtN2.txt


